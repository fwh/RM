LET sumMulThreeFive(n, i, sum) = IF i < n THEN doSum(n, i, sum) ELSE sum;
LET doSum(n, i, sum) = IF i % 3 == 0 || i % 5 == 0 THEN sumMulThreeFive(n, i+1, sum+i) ELSE sumMulThreeFive(n, i+1, sum);
